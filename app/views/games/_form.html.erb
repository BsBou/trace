<%= simple_form_for @game do |f| %>
  <%= f.input :name, as: :string %>
  <%= f.input :city, as: :string %>
  <%= f.input :start, as: :datetime, html5: true%>
  <%= f.input :end, as: :datetime, html5: true %>
  <%= f.input :photo, as: :string %>
  <%= f.button :submit, class: "button beige" %>
<% end %>
